<Alloy>
	<Window id="listaUsuarios" layout="vertical">
		<View id="header" layout="horizontal">
			<Button onClick="doBack">Atras</Button>
			<Label id="title" textAlign="center">Usuarios</Label>
			<View class="divider"/>
			<View id="addView">
				<ImageView id="addImage"/>
			</View>
		</View>
		<View>
			<TableView id="lista" layout="vertical">
				<TableViewRow layout="horizontal">
					<ImageView image="/logo.png"></ImageView>
					<Label>Texto</Label>
				</TableViewRow>
			</TableView>
		</View>
	</Window>
</Alloy>